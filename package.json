{
  "name": "huskutlir",
  "version": "1.0.0",
  "description": "A multiplayer reversi game",
  "private": true,
  "scripts": {
    "test": "jest",
    "build": "webpack --mode production",
    "start": "npm run build && node src/server/server.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "standard": {
    "env": [
      "jest"
    ],
    "parser": "babel-eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/olif/huskutlir.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/olif/huskutlir/issues"
  },
  "homepage": "https://github.com/olif/huskutlir#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "shortid": "^2.2.14",
    "styled-components": "^4.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.0",
    "babel-plugin-styled-components": "^1.8.0",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^4.0.1",
    "css-loader": "^0.28.11",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "standard": "^12.0.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.12.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.10",
    "ws": "^6.1.0"
  }
}
